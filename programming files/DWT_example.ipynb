   "source": [
    "import numpy as np\n",
    "import pywt\n",
    "\n",
    "f = np.array([14,8,6,4,3,5,9,7])\n",
    "\n",
    "print(\"Haar Wavelet\")\n",
    "cA,cD = pywt.dwt(f,'db1')\n",
    "print(\"DWT Coefficients: \",cA,cD)\n",
    "fp = pywt.idwt(cA,cD,'db1')\n",
    "print(\"Reconstruction\",fp)"
   ]
